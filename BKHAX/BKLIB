		Section2:Destroy()
	end)
	
	hide.MouseButton1Click:Connect(function()
		ripple(hide,0,0,0)
		if sec_Vis == true then
			--print("Hide")
			sec_Vis = false
			sec_frame:WaitForChild(selectedtab):TweenPosition(UDim2.new(0.241, 390,0.1, 0),1,5,0.3)
			sideBar:TweenPosition(UDim2.new(0,-120,0.1,0),1,5,0.3)
			wait(0.3)
			section:TweenSize(UDim2.new(0,500,0,30),1,5,0.3)
			wait(0.3)
			section:TweenSize(UDim2.new(0,300,0,30),1,5,0.5)
			hide:TweenPosition(UDim2.new(0.54, 0,0, 0),1,5,0.5)
