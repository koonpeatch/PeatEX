		function library:AddTab(name,canvasSizeY)	
			canvasSizeY = tostring(canvasSizeY or 0)
			--Virble
			local fparent = section
			local tab_comp = {}
			local len = (#menu_list:GetChildren() - 0) *32
			menu_list.CanvasSize = UDim2.new(0,0,0,len)
			
			--TabButton
			tab = tab + 1
			local tabname = "Tab_"..tab
			local btn = Instance.new("TextButton")
			btn.Parent = menu_list
			btn.Text = name
			btn.ClipsDescendants = true
			btn.BackgroundTransparency = 1
			btn.TextColor3 = Color3.fromRGB(255,255,255)
			btn.Font = Enum.Font.Gotham
			btn.TextSize = 12
			btn.TextWrapped = true
			btn.TextXAlignment = Enum.TextXAlignment.Left
			
			--When Click on btn
			btn.MouseButton1Down:Connect(function()
				ripple(btn,22,155,22)
				if selectedtab == tabname then
					--print("This is TAB!!")
				else
					for i,v in pairs(sec_frame:GetChildren()) do
						
						if v.Name == tabname or v.Name == "SideBar" or v.Name == "TopBar" then
							v.Visible = true
							if v.Name == tabname then
								v.Position = UDim2.new(0.241, 390,0.1, 0)
								v:TweenPosition(UDim2.new(0.241, 0,0.1, 0),1,5,0.3)
							else
								
							end
						else
							v.Visible = false
						end
					end
				end
				--print(tabname)
				selectedtab = tabname
			end)
			
			--CreatePanel
			local select_frame = Instance.new("Frame")
			local scframe = Instance.new("ScrollingFrame")
			local UILay = Instance.new("UIGridLayout")
			select_frame.Name = tabname
			select_frame.Parent = fparent
			select_frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			select_frame.BackgroundTransparency = 1
			select_frame.Position = UDim2.new(0.240999997, 0, 0.100000001, 0)
			select_frame.Size = UDim2.new(0, 379, 0, 269)
			select_frame.Visible = false
			
			
			scframe.Name = "scframe"
			scframe.Parent = select_frame
			scframe.Active = true
			scframe.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			scframe.BackgroundTransparency = 1.000
			scframe.BorderSizePixel = 0
			scframe.Size = UDim2.new(0, 379, 0, 269)
			scframe.ScrollBarThickness = 5
			scframe.CanvasSize = UDim2.new(0,0,canvasSizeY,0)
			
			UILay.Name = "UILay"
			UILay.Parent = scframe
			UILay.HorizontalAlignment = Enum.HorizontalAlignment.Center
			UILay.SortOrder = Enum.SortOrder.LayoutOrder
			UILay.CellSize = UDim2.new(0, 350, 0, 30)
		
		
			if tabname == selectedtab then
				select_frame.Visible = true
			end
			
