					TextButton.TextSize = 13.000
					TextButton.Text = name
					TextButton.ClipsDescendants = true
					
					TextButton.MouseButton1Click:Connect(function() 
						dp_show = false
						ripple(TextButton,155,155,155)
						Dropdown_Button.Text = TextButton.Text
						pcall(callback ,TextButton.Text)
						Dropdown_List:TweenSize(UDim2.new(0, 128,0, 0),1,5,0.3)
						Icon_Button.Rotation = 90
						for i,v in pairs(section[tabname].scframe:GetChildren()) do
							if v.Name == "UILay" then
								
							elseif v.Name == dp_name then
								
							else
								v.Visible = true
								scframe.CanvasSize = UDim2.new(0,0,canvasSizeY,0)
								scframe.CanvasPosition = shared.cpost
							end
						end
					end)
				end
				
				function dropdown_data:ClearAll()
					for i,v in pairs(ScroolingF:GetChildren()) do
						if v:IsA("UIGridLayout") then
							
						else
							v:Destroy()
						end
					end
				end
				
				return dropdown_data
			end
			
			function tab_comp:AddSlider(slider_text, set_val, callback)
				slider_text = tostring(slider_text or "New Slider")
				callback = typeof(callback) == "function" and callback or function()end
				set_val = {
					["min"] = set_val.min or 0,
					["max"] = set_val.max or 100,
					["set"] = set_val.set or 0
				}
				
				local Value = set_val.set
				minvalue = set_val.min
				maxvalue = set_val.max
				
				local Slider = Instance.new("Frame")
				local TextLabel = Instance.new("TextLabel")
				local SlideBG = Instance.new("ImageLabel")
				local TextButton = Instance.new("TextButton")
				local ValS = Instance.new("ImageLabel")
				local Value1 = Instance.new("TextLabel")
				
				--Properties:
				
				Slider.Name = "Slider"
				Slider.Parent = scframe
				Slider.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				Slider.BackgroundTransparency = 1.000
				Slider.BorderSizePixel = 0
				Slider.Size = UDim2.new(0, 100, 0, 100)
				
				TextLabel.Parent = Slider
				TextLabel.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
				TextLabel.BackgroundTransparency = 1.000
				TextLabel.BorderSizePixel = 0
				TextLabel.Size = UDim2.new(0, 86, 0, 30)
				TextLabel.Font = Enum.Font.GothamBold
				TextLabel.Text = slider_text
				TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
				TextLabel.TextSize = 16.000
				TextLabel.TextStrokeColor3 = Color3.fromRGB(255, 255, 255)
				TextLabel.TextXAlignment = Enum.TextXAlignment.Left
				
				SlideBG.Name = "SlideBG"
				SlideBG.Parent = Slider
				SlideBG.AnchorPoint = Vector2.new(0, 0.5)
				SlideBG.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				SlideBG.BackgroundTransparency = 1.000
				SlideBG.Position = UDim2.new(0.632000029, 0, 0, 15)
				SlideBG.Size = UDim2.new(0, 130, 0, 13)
				SlideBG.Image = "rbxassetid://3570695787"
				SlideBG.ImageColor3 = Color3.fromRGB(59, 59, 59)
				SlideBG.ScaleType = Enum.ScaleType.Slice
				SlideBG.SliceCenter = Rect.new(100, 100, 100, 100)
				SlideBG.SliceScale = 0.120
				
				TextButton.Parent = SlideBG
				TextButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				TextButton.BackgroundTransparency = 1.000
				TextButton.BorderSizePixel = 0
				TextButton.Size = UDim2.new(0, 130, 0, 13)
				TextButton.Font = Enum.Font.SourceSans
				TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
				TextButton.TextSize = 14.000
				TextButton.TextTransparency = 1.000
				
				ValS.Name = "ValS"
				ValS.Parent = TextButton
				ValS.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				ValS.BackgroundTransparency = 1.000
				ValS.BorderSizePixel = 0
				ValS.Size = UDim2.new(0, 130, 0, 13)
				ValS.Image = "rbxassetid://3570695787"
				ValS.ImageColor3 = Color3.fromRGB(155, 55, 55)
				ValS.ScaleType = Enum.ScaleType.Slice
				ValS.SliceCenter = Rect.new(100, 100, 100, 100)
				ValS.SliceScale = 0.120
				
				Value1.Name = "Value"
				Value1.Parent = Slider
				Value1.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
				Value1.BackgroundTransparency = 1.000
				Value1.BorderSizePixel = 0
				Value1.Position = UDim2.new(0.37372607, 0, 0, 0)
				Value1.Size = UDim2.new(0, 86, 0, 30)
				Value1.Font = Enum.Font.Gotham
				Value1.Text = "0"
				Value1.TextColor3 = Color3.fromRGB(255, 255, 255)
				Value1.TextSize = 13.000
				Value1.TextStrokeColor3 = Color3.fromRGB(255, 255, 255)
				Value1.TextXAlignment = Enum.TextXAlignment.Right
				
				local wight = 130
				local height = 13
				
				
				TextButton.MouseButton1Down:Connect(function()
					Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / wight) * ValS.AbsoluteSize.X) + tonumber(minvalue)) or 0
					pcall(function()
						callback(Value)
					end)
					ValS.Size = UDim2.new(0, math.clamp(mouse.X - ValS.AbsolutePosition.X, 0, wight), 0, height)
					moveconnection = mouse.Move:Connect(function()
						Value1.Text = Value
						Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / wight) * ValS.AbsoluteSize.X) + tonumber(minvalue))
						pcall(function()
							callback(Value)
						end)
						ValS.Size = UDim2.new(0, math.clamp(mouse.X - ValS.AbsolutePosition.X, 0, wight), 0, height)
					end)
					releaseconnection = UIS.InputEnded:Connect(function(Mouse)
						if Mouse.UserInputType == Enum.UserInputType.MouseButton1 then
							Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / wight) * ValS.AbsoluteSize.X) + tonumber(minvalue))
							pcall(function()
								callback(Value)
							end)
							ValS.Size = UDim2.new(0, math.clamp(mouse.X - ValS.AbsolutePosition.X, 0, wight), 0, height)
							moveconnection:Disconnect()
							releaseconnection:Disconnect()
						end
					end)
				end)
			end
			
			function tab_comp:AddKeyBlind(Key_blind_text,callback)
				local kb_comp = {}
				local check_blinding = false
				Key_blind_text = tostring(Key_blind_text or "New Blind")
				callback = typeof(callback) == "function" and callback or function()end
				
				
				local Blind = Instance.new("Frame")
				local TextLabel = Instance.new("TextLabel")
				local Input = Instance.new("ImageLabel")
				local TextButton = Instance.new("TextButton")
				
				--Properties:
				
				Blind.Name = "Blind"
				Blind.Parent = scframe
				Blind.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				Blind.BackgroundTransparency = 1.000
				Blind.BorderSizePixel = 0
				Blind.Size = UDim2.new(0, 100, 0, 100)
				
				TextLabel.Parent = Blind
				TextLabel.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
				TextLabel.BackgroundTransparency = 1.000
				TextLabel.BorderSizePixel = 0
				TextLabel.Size = UDim2.new(0, 200, 0, 30)
				TextLabel.Font = Enum.Font.GothamBold
				TextLabel.Text = Key_blind_text
				TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
				TextLabel.TextSize = 16.000
				TextLabel.TextStrokeColor3 = Color3.fromRGB(255, 255, 255)
				TextLabel.TextXAlignment = Enum.TextXAlignment.Left
				
				Input.Name = "Input"
				Input.Parent = Blind
				Input.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				Input.BackgroundTransparency = 1.000
				Input.ClipsDescendants = true
				Input.Position = UDim2.new(0.695360482, 0, 0, 0)
				Input.Size = UDim2.new(0, 108, 0, 30)
				Input.Image = "rbxassetid://3570695787"
				Input.ImageColor3 = Color3.fromRGB(59, 59, 59)
				Input.ScaleType = Enum.ScaleType.Slice
				Input.SliceCenter = Rect.new(100, 100, 100, 100)
				Input.SliceScale = 0.120
				
				TextButton.Parent = Input
				TextButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				TextButton.BackgroundTransparency = 1.000
				TextButton.BorderSizePixel = 0
				TextButton.Size = UDim2.new(0, 106, 0, 30)
				TextButton.Font = Enum.Font.Gotham
				TextButton.Text = "> UnSet <"
				TextButton.TextColor3 = Color3.fromRGB(178, 178, 178)
				TextButton.TextSize = 13.000
				
				local shortkeys = { -- Thx someone
					RightControl = 'RightCtrl',
					LeftControl = 'LeftCtrl',
					LeftShift = 'LShift',
					RightShift = 'RShift',
					MouseButton1 = "Mouse1",
					MouseButton2 = "Mouse2"
				}
				
				local keycode = 0
				
				UIS.InputBegan:Connect(function(a,b)
					if check_blinding then
						check_blinding = false
						return
					end
					if a.KeyCode == keycode and not b then
						pcall(callback, keycode)
					end
				end)
				
				function kb_comp:SetButton(Btn_Name)
					local key = shortkeys[Btn_Name.Name] or Btn_Name.Name
					check_blinding = true
					TextButton.Text = "> "..key.." <"
					keycode = Btn_Name
				end
				
				TextButton.MouseButton1Click:Connect(function()
					ripple(TextButton,155,155,155)
					TextButton.Text = "> ... <"
					local a,b = UIS.InputBegan:Wait()
					kb_comp:SetButton(a.KeyCode)
					
				end)
				return kb_comp
			end
	
			
			--return
			return tab_comp
		end
	end
	
