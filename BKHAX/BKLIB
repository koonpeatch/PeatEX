local function RPDBZW_fake_script() -- TextLabel.LocalScript
	local script = Instance.new('LocalScript', TextLabel)
	TextLabel2.Position = UDim2.new(-0.2500000007, 0, 0.569999976, 0)
	TextLabel2:TweenPosition(UDim2.new(0.0500000007, 0,0.569999976,0),1,5,0.5)
	wait(0.2)
	
	TextLabel.Position = UDim2.new(-0.2500000007, 0, 0.599999976, 0)
	TextLabel:TweenPosition(UDim2.new(0.0500000007, 0,0.599999976,0),1,5,0.5)
	wait(0.2)
	
	TextLabel3.Position = UDim2.new(-0.2500000007, 0, 0.629999976, 0)
	TextLabel3:TweenPosition(UDim2.new(0.0500000007, 0,0.629999976,0),1,5,0.5)
	wait(0.2)
	wait(5)
	TextLabel:TweenPosition(UDim2.new(-0.3500000007, 0,0.599999976,0),1,5,0.5)
	TextLabel2:TweenPosition(UDim2.new(-0.3500000007, 0,0.569999976,0),1,5,0.5)
	TextLabel3:TweenPosition(UDim2.new(-0.3500000007, 0,0.629999976,0),1,5,0.5)
	wait(0.5)
	Creditez:Destroy()
	
end
