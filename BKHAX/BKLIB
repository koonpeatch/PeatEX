		else
			--print("Show")
			sec_Vis = true
			section:TweenSize(UDim2.new(0,500,0,30),2,5,0.3)
			hide:TweenPosition(UDim2.new(0.885, 0,0, 0),2,5,0.3)
			wait(0.3)
			section:TweenSize(UDim2.new(0,500,0,300),2,5,0.3)
			wait(0.3)
			sec_frame:WaitForChild(selectedtab):TweenPosition(UDim2.new(0.241, 0,0.1, 0),2,5,0.3)
			sideBar:TweenPosition(UDim2.new(0,0,0.1,0),2,5,0.3)
			wait(0.3)
		end
	end)
	
	
	--[[ FUNCTION ]]--
	function ripple(button,r,g,b)
		local ms = game.Players.LocalPlayer:GetMouse()
		local rippleTex = Ripple_2
		rippleTex.ImageColor3 = Color3.fromRGB(r,g,b)
		local c = rippleTex:Clone()
		c.Parent = button
		local x, y = (ms.X - c.AbsolutePosition.X), (ms.Y - c.AbsolutePosition.Y)
		c.Position = UDim2.new(0, x, 0, y)
		local len, size = 0.35, nil
		if button.AbsoluteSize.X >= button.AbsoluteSize.Y then
			size = (button.AbsoluteSize.X * 1.5)
		else
			size = (button.AbsoluteSize.Y * 1.5)
		end
		c:TweenSizeAndPosition(UDim2.new(0, size, 0, size), UDim2.new(0.5, (-size / 2), 0.5, (-size / 2)), 'Out', 'Quad', len, true, nil)
		for i = 1, 10 do
			c.ImageTransparency = c.ImageTransparency + 0.05
			wait(len / 12)
		end
		c:Destroy()
	end
	
	
