                                until not v:FindFirstChildWhichIsA("Model") or nearest.Parent == nil or on2 == false
                                print("next mob please")
                                last, nearest = math.huge, nil
                                twen:Cancel()
                            end
                        end
                    end
                end
            end
        else
            last, nearest = math.huge, nil
        end
    end
end)
